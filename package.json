{
    "name": "numlab-highlight",
    "displayName": "NumLab Highlight Extension",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "publisher": "jessicanunez",
    "author": {
        "name": "Jessica Nunez"
    },
    "homepage": "https://github.com/jessicanhdez/numlab-highlight.git",
    "repository": {
        "type": "git",
        "url": "https://github.com/jessicanhdez/numlab-highlight.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "numlab",
        "highlight"
    ],
    "contributes": {
        "languages": [{
            "id": "nlh",
            "aliases": ["NumLab", "nlh"],
            "extensions": [".nl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nlh",
            "scopeName": "source.nlh",
            "path": "./syntaxes/nlh.tmLanguage.json"
        }]
    }
}